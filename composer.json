{
	"name": "queryr/serialization",
	"require": {
		"php": ">=5.5.0",

		"wikibase/data-model": "dev-master",
		"serialization/serialization": "~3.2",

		"queryr/term-store": "dev-master"
	},
	"require-dev": {
		"whatthejeff/nyancat-phpunit-resultprinter": "~1.2"
	},
	"autoload": {
		"psr-4": {
			"Queryr\\Serialization\\": "src/Serialization/",
			"Queryr\\Resources\\": "src/Resources/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.1.x-dev"
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:Queryr/TermStore.git"
		}
	]
}
